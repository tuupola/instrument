{
    "name": "tuupola/instrument",
    "description": "Application metrics toolbox for InfluxDB",
    "keywords": [
        "influxdb",
        "metrics"
    ],
    "homepage": "https://github.com/tuupola/instrument",
    "license": "MIT",
    "authors": [
        {
            "name": "Mika Tuupola",
            "email": "tuupola@appelsiini.net",
            "homepage": "http://www.appelsiini.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0|^8.0",
        "influxdb/influxdb-php": "^1.2",
        "symfony/stopwatch": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "squizlabs/php_codesniffer": "^2.5",
        "klaussilveira/simple-shm": "^1.0"
    },
    "suggest": {
        "klaussilveira/simple-shm": "If you want to use the Gauge datatype."
    },
    "autoload": {
        "psr-4": {
            "Instrument\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Instrument\\": "tests"
        }
    }
}
